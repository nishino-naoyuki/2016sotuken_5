<?xml version="1.0" encoding="UTF-8" ?>
<web-app>
<filter>
	<filter-name>EncodingFilter</filter-name>
	<filter-class>jp.ac.asojuku.food.filter.EncodingFilter</filter-class>
</filter>

<filter-mapping>
	<filter-name>EncodingFilter</filter-name>
	<url-pattern>/*</url-pattern>
</filter-mapping>


<servlet>
	<servlet-name>UserAddAction</servlet-name>
	<servlet-class>jp.ac.asojuku.food.action.UserAddAction</servlet-class>
</servlet>
<servlet>
	<servlet-name>UserAddCheckAction</servlet-name>
	<servlet-class>jp.ac.asojuku.food.action.UserAddCheckAction</servlet-class>
</servlet>
<servlet>
	<servlet-name>UserAddComp</servlet-name>
	<servlet-class>jp.ac.asojuku.food.action.UserAddComp</servlet-class>
</servlet>



<servlet-mapping>
	<servlet-name>UserAddAction</servlet-name>
	<url-pattern>/useradd</url-pattern>
</servlet-mapping>
<servlet-mapping>
	<servlet-name>UserAddCheckAction</servlet-name>
	<url-pattern>/usercheck</url-pattern>
</servlet-mapping>
<servlet-mapping>
	<servlet-name>UserAddComp</servlet-name>
	<url-pattern>/usercomp</url-pattern>
</servlet-mapping>
</web-app>